FUNCTION "osr.edw.staging.md.rms.bp.proxy::TF_BP_CONVERSION_OUTPUT"(IV_BPARTNER NVARCHAR(10))
       RETURNS "BPARTNER"	NVARCHAR(10)
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 

DECLARE LV_BPARTNER NVARCHAR(10);

IF :IV_BPARTNER IS NULL OR :IV_BPARTNER = '' THEN 
	BPARTNER := '';
ELSE
	SELECT LTRIM(:BPARTNER, '0') INTO BPARTNER FROM "osr.hana.platform.synonym::SYS.DUMMY";
END IF;
		
END;
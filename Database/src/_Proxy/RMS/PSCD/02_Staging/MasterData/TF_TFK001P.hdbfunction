FUNCTION "osr.edw.staging.md.rms.pscd.proxy::TF_TFK001P"( )
RETURNS TABLE (
	PERSL		NVARCHAR(4),
	TXT50		NVARCHAR(50)
)
LANGUAGE SQLSCRIPT 
SQL SECURITY INVOKER AS 
BEGIN 

RETURN
SELECT 
	"TFK001P"."PERSL",
	"TFK001PT"."TXT50"
FROM 
"osr.edw.source.md.rms.synonym::CDS_PSCD.TAB.TFK001P" AS "TFK001P"
LEFT OUTER JOIN
 "osr.edw.source.md.rms.synonym::CDS_PSCD.TAB.TFK001PT" AS "TFK001PT"
 ON "TFK001P"."PERSL" = "TFK001PT"."PERSL"
 WHERE "TFK001PT"."SPRAS" ='E'
;


END;
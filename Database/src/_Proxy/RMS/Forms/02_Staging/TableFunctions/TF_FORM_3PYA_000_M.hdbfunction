MAIN.C_F_LAST_PER_AMNT,
MAIN.C_F_LAST_PER_START,
MAIN.C_F_TOTAL_PER_END,
MAIN.C_P_AND_P_END_DATE,
MAIN.C_F_RESID_START,
MAIN.C_DEDUCTION_ALLOWED,
MAIN.C_TAX_AMOUNT,
MAIN.C_BASE_TAX,
MAIN.C_REBATE_NETT,
MAIN.C_PAY_TAX_AMOUNT,
MAIN.C_TOT_PAID_PAYABLE,
MAIN.C_PAY_TAX_LIAB_CREDIT,

MAIN.C_CURRENCY
FROM "osr.edw.source.td.rms.synonym::CDS_TRM.DSO.DS_3PSCD_TRM_FORM_DATA_3PYA.active_data" AS MAIN
WHERE MAIN.FIELDINDEX = 0

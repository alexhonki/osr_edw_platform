C_F_RESID_START NVARCHAR(8),
C_DEDUCTION_ALLOWED DECIMAL(23,4),
C_TAX_AMOUNT DECIMAL(23,4),
C_BASE_TAX DECIMAL(23,4),
C_REBATE_NETT DECIMAL(23,4),
C_PAY_TAX_AMOUNT DECIMAL(23,4),
C_TOT_PAID_PAYABLE DECIMAL(23,4),
C_PAY_TAX_LIAB_CREDIT DECIMAL(23,4),
A_CURRENCY NVARCHAR(5),
C_CURRENCY NVARCHAR(5)
)
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 
/**********************************************************************************************
 ISR scenario 3PYA | scenario ver. 000 | main table | form ver. all
 **********************************************************************************************/ 
RETURN
SELECT
MAIN.RETURN_ID,
MAIN.FORM,
MAIN.FORM_NUMBER,
MAIN.CASE_GUID,
MAIN.FIELDINDEX,
MAIN.TIMESTAMP,
MAIN.FORM_CLASS,
MAIN.FORM_STATUS,
MAIN.FORM_STATUS_TS,
MAIN.SUPERSEDED_BY,
MAIN.SUPERSEDE_TO,
MAIN.RECEIPT_TS,
MAIN.TEMPLATE_VER,
MAIN.BATCH_ID,
MAIN.LOCATOR_NUMBER,
MAIN.FBTYP,
MAIN.FBNUM,
'00001' AS VERSION,
MAIN.A_GROSS_SAL_WAGE,
MAIN.A_ALLOWANCE,
MAIN.A_BONUS,
MAIN.A_COMMISSION,
MAIN.A_DIRECTOR_FEE,
MAIN.A_EMPL_SUP_CONTRIB,
MAIN.A_FRINGE_BENEFIT,
MAIN.A_ELIG_TERM_PMT,
MAIN.A_CONTRACTORS,
MAIN.A_SHARE_SCHEMES,
MAIN.A_APPRENT_PMT,
MAIN.A_TRAINEES_PMT,
MAIN.A_OTH_NON_TAX_WAGE,
CASE WHEN MAIN.A_INT_WAGES_Q = '-' THEN 'N'
ELSE MAIN.A_INT_WAGES_Q
END AS A_INT_WAGES_Q,

A_REASS_EVENT NVARCHAR(50),
A_EVENT_DATE NVARCHAR(8),
A_DISP_DATE_REASS NVARCHAR(8),
C_SENDER_ID NVARCHAR(10),
C_ELN_ID NVARCHAR(36)
)
LANGUAGE SQLSCRIPT 
SQL SECURITY INVOKER AS 
BEGIN

/**********************************************************************************************
 ISR scenario 0IDN | scenario ver. 001 | main table | form ver. latest
 **********************************************************************************************/ 

RETURN
SELECT
FORM.MANDT,
FORM.RETURN_ID,
FORM.FORM,
FORM.FORM_NUMBER,
FORM.CASE_GUID,
FORM.FIELDINDEX,
FORM.TIMESTAMP,
FORM.FORM_CLASS,
FORM.FORM_STATUS,
FORM.FORM_STATUS_TS,
FORM.SUPERSEDED_BY,
FORM.SUPERSEDE_TO,
FORM.RECEIPT_TS,
FORM.TEMPLATE_VER,
FORM.BATCH_ID,
FORM.LOCATOR_NUMBER,
FORM.VERSION,
FORM.OBJECT_ID,
FORM.C_BP,
FORM.C_BPNAME,
FORM.C_FBN,
FORM.C_ELN_TXN,
FORM.C_LODGE_DATE,
FORM.A_YOUR_REF,
FORM.A_ILODGE_DATE,
FORM.A_TCLASS_TD,
FORM.A_DTYPE,
FORM.A_DPROP,
FORM.A_TCLASS_LR,
FORM.A_INS_TYPE,
FORM.A_TRANS_DATE,
FORM.A_UNCOND_DATE,

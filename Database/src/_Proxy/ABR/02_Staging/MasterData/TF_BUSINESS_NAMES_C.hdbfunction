FUNCTION "osr.edw.staging.md.abr.proxy::TF_BUSINESS_NAMES_C"( )
RETURNS TABLE (
"PID" INTEGER,
"BUS_NM" NVARCHAR(200)
)
LANGUAGE SQLSCRIPT 
SQL SECURITY INVOKER AS 
BEGIN 

RETURN 
SELECT
	"PID",
	"BUS_NM"
FROM "osr.edw.staging.md.abr.synonym::CDS_ORG.DSO.BUSINESS_NAMES.active_data"
WHERE "Z_CURR_FLAG" = 1;

END;

FUNCTION "osr.edw.platform.fra.prt.dt.abn.vh::TF_ANZSIC_CODE"( )
RETURNS TABLE (
	ANZSIC_CD		NVARCHAR(5),
	ANZSIC_CD_DESCR	NVARCHAR(100)
)
LANGUAGE SQLSCRIPT 
SQL SECURITY INVOKER AS 
BEGIN 

RETURN 
SELECT 
DISTINCT
	"MN_INDY_CLSN" AS "ANZSIC_CD",
	"MN_INDY_CLSN_DESCN" AS "ANZSIC_CD_DESCR"
FROM "osr.edw.staging.md.abr.synonym::CDS_ORG.DSO.AGENCY_DATA.active_data"
;
END;
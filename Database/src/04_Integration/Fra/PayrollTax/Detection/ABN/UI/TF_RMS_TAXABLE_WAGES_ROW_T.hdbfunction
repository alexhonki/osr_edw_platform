FUNCTION "osr.edw.platform.fra.prt.dt.abn.ui::TF_RMS_TAXABLE_WAGES_ROW_T"( )
/**********
	CREATED BY		: LAKSHMI
	CREATED ON		: 04/03/2019
	CREATION REASON	: Having a measure field text mapping function. 
**********/
	RETURNS TABLE 
	(
	MEASURE_NAME	NVARCHAR(30),
	LANGU			NVARCHAR(1),
	MEASURE_TEXT	NVARCHAR(100)
	)
LANGUAGE SQLSCRIPT 
SQL SECURITY INVOKER AS 
BEGIN 

	lt_dummy = 
	SELECT TOP 1 * 
	FROM "osr.edw.staging.md.rms.synonym::CDS_PSCD.DSO.T009.active_data" AS "T009";

RETURN 
	SELECT 
		'CLIENT_NAME'		 					AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Organisation Name'						AS "MEASURE_TEXT"
	FROM :lt_dummy
	UNION
	SELECT 
		'ABN' 									AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'ABN'									AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'ACN' 									AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'ACN'									AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'STATE_CLIENT_ID'	 					AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Customer ID'							AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'STATE_GROUP_ID'	 					AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Group ID'								AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'CLIENT_STATE_WAGES' 					AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Client_State_Wages'					AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'CLIENT_INTERSTATE_WAGES' 				AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Client_Interstate_Wages'				AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_ALLOWANCE' 						AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Allowance'						AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_APPRENTICE' 						AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Apprentice'						AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_BONUS' 							AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Bonus'							AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_COMMISSION' 						AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Commission'						AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_CONTRACTOR' 						AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Contractor'						AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_DIRECTOR' 						AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Director'							AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_TERMINATION' 						AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Termination'						AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_FB' 								AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_FB'								AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_SALARY' 							AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Salary'							AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_SUPER' 							AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Super'							AS "MEASURE_TEXT"
	FROM :lt_dummy
		UNION
	SELECT 
		'WAGE_OTHER' 							AS "MEASURE_NAME",
		'E' 									AS "LANGU",
		'Wage_Other'							AS "MEASURE_TEXT"
	FROM :lt_dummy
	
	;

END;
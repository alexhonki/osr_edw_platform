FUNCTION "osr.edw.platform.fra.prt.model::TF_GRP_GROUP_EDGES"(IV_GRP_DATE DATE, IV_GRAPH_ID NVARCHAR(32) )
RETURNS TABLE (
	"GRP_DATE"				DATE,
    "CHILD_ID"				NVARCHAR(32),
	"PARENT_ID"				NVARCHAR(32),
	"REL_TYPE"				NVARCHAR(2),
	"GRAPH_ID"				NVARCHAR(32)
	
)
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 

RETURN
SELECT
"GRP_DATE",
"CHILD_ID",
"PARENT_ID",
'DR' AS "REL_TYPE",
"GRAPH_ID"
FROM
"osr.edw.platform.fra.prt.model::TF_GRP_GROUP_DR_REL"(:IV_GRP_DATE)
WHERE :IV_GRAPH_ID = '' OR "GRAPH_ID" = :IV_GRAPH_ID 
UNION
SELECT
"GRP_DATE",
"CHILD_ID",
"PARENT_ID",
'SH' AS "REL_TYPE",
"GRAPH_ID"
 FROM
 "osr.edw.platform.fra.prt.model::TF_GRP_GROUP_SH_REL"(:IV_GRP_DATE)
 WHERE :IV_GRAPH_ID = '' OR "GRAPH_ID" = :IV_GRAPH_ID
;

END;
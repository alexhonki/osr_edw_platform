PROCEDURE "osr.edw.platform.fra.ltx.coowner.test::SP_ParcelBPToCoOwnerTotValTest" (
	IN	IV_TAXYRSTARTDT						NVARCHAR(8),
	IN	IV_PLNO								NVARCHAR(20),
	IN	IV_AVG3YRVAL_THRESHOLD				Decimal(15, 2),
	OUT ET_RESULT							"osr.edw.platform.fra.ltx.coowner::Types.PCL_TOT_VAL_RESULT",
	OUT ET_TEXT  							"osr.edw.platform.fra.ltx.coowner::Types.TEXT_LTPCLBP"
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	READS SQL DATA AS
BEGIN

	
	LT_WORKING_SET = 
	SELECT
	"TAXYRSTARTDT",
    "INTRENOPL",
    "PLNO",
    "BPARTNER",
    "VALID_FROM",
    "VALID_TO"
	FROM "osr.edw.platform.fra.ltx.coowner::TF_ParcelBPDetObj"()
	WHERE "PLNO" = :IV_PLNO
	  AND "TAXYRSTARTDT" >= :IV_TAXYRSTARTDT
	;    
    
  CALL "osr.edw.platform.fra.ltx.coowner.se::SP_ParcelBPSele"(
    :LT_WORKING_SET,
    LT_OUTPUT_READ );

    LT_PARAM_DM =
      SELECT
        :IV_AVG3YRVAL_THRESHOLD	AS "AVG3YRVAL_THRESHOLD"
    FROM "osr.hana.platform.synonym::SYS.DUMMY";
    
  CALL  "osr.edw.platform.fra.ltx.coowner.ex::SP_ParcelBPToCoOwnerTotVal"(
    :LT_OUTPUT_READ,
    :LT_PARAM_DM,
    ET_RESULT,
    ET_TEXT );

 END;
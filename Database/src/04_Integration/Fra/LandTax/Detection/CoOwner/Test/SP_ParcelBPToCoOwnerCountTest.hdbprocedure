PROCEDURE "osr.edw.platform.fra.ltx.coowner.test::SP_ParcelBPToCoOwnerCountTest" (
	IN	IV_TAXYRSTARTDT						NVARCHAR(8),
	IN	IV_PLNO								NVARCHAR(20),
	IN	IV_OWNER_COUNT_THRESHOLD			Integer,
	IN	IV_ORG_COUNT_RANGE_LOW				Integer,
	IN	IV_ORG_COUNT_RANGE_HIGH				Integer,
	IN	IV_TRT_COUNT_THRESHOLD				Integer,
	IN	IV_COY_COUNT_THRESHOLD				Integer,
	IN	IV_IND_COUNT_THRESHOLD				Integer,
	OUT ET_RESULT							"osr.edw.platform.fra.ltx.coowner::Types.RESULT_LTPCLBP",
	OUT ET_TEXT  							"osr.edw.platform.fra.ltx.coowner::Types.TEXT_LTPCLBP"
	)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	READS SQL DATA AS
BEGIN

	
	LT_WORKING_SET = 
	SELECT
    "INTRENOPL",
    "PLNO",
    "BPARTNER",
    "VALID_FROM",
    "VALID_TO"
	FROM "osr.edw.platform.fra.ltx.coowner::TF_ParcelBPDetObj"()
	WHERE "PLNO" = :IV_PLNO
	  AND "VALID_FROM" >= :IV_TAXYRSTARTDT
	;    
    
  CALL "osr.edw.platform.fra.ltx.coowner.se::SP_ParcelBPSele"(
    :LT_WORKING_SET,
    LT_OUTPUT_READ );

    LT_PARAM_DM =
      SELECT
        :IV_OWNER_COUNT_THRESHOLD	AS "OWNER_COUNT_THRESHOLD",
        :IV_ORG_COUNT_RANGE_LOW     AS "ORG_COUNT_RANGE_LOW",
        :IV_ORG_COUNT_RANGE_HIGH    AS "ORG_COUNT_RANGE_HIGH",
        :IV_TRT_COUNT_THRESHOLD     AS "TRT_COUNT_THRESHOLD",
        :IV_COY_COUNT_THRESHOLD     AS "COY_COUNT_THRESHOLD",
        :IV_IND_COUNT_THRESHOLD     AS "IND_COUNT_THRESHOLD"
    FROM "osr.hana.platform.synonym::SYS.DUMMY";
    
  CALL  "osr.edw.platform.fra.ltx.coowner.ex::SP_ParcelBPToCoOwnerCount"(
    :LT_OUTPUT_READ,
    :LT_PARAM_DM,
    ET_RESULT,
    ET_TEXT );

 END;